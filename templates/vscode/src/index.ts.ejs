import * as vscode from 'vscode'

// activate方法会在插件被激活时调用
export function activate(context: vscode.ExtensionContext) {
// 注册命令，第一个参数是命令名称，第二参数是回调
const disposable = vscode.commands.registerCommand('<%= projectName %>.helloWorld', () => {
  // 弹出消息提示
  vscode.window.showInformationMessage('Hello World from <%= projectName %>!')
    })

    // 添加到插件上下文
    context.subscriptions.push(disposable)
    }

    // deactivate方法会在插件失活时调用
    export function deactivate(context: vscode.ExtensionContext) {
    context.subscriptions.forEach((d) => d.dispose())
    }